{
    "multipart": [
        {   
            "when": { "up": "true" },
            "apply": { "model": "minestrapp:terracreep_up" }
        },
        {   
            "when": { "down": "true" },
            "apply": { "model": "minestrapp:terracreep_up", "x": 180 }
        },
        {   
            "when": { "south": "true" },
            "apply": { "model": "minestrapp:terracreep_up", "x": 90, "y": 180 }
        },
        {   
            "when": { "north": "true" },
            "apply": { "model": "minestrapp:terracreep_up", "x": 90 }
        },
        {   
            "when": { "west": "true" },
            "apply": { "model": "minestrapp:terracreep_up", "x": 90, "y": 270 }
        },
        {   
            "when": { "east": "true" },
            "apply": { "model": "minestrapp:terracreep_up", "x": 90, "y": 90 }
        }  
    ]
}
