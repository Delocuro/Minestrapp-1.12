{
    "multipart": [
        {   
            "when": { "connected": "0|3|4", "facing": "north" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz" }
        },
        {   
            "when": { "connected": "0|1|3", "facing": "east" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz" , "y": 90, "uvlock": true}
        },
        {   
            "when": { "connected": "0|1|2", "facing": "south" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz" , "y": 180, "uvlock": true}
        },
        {   
            "when": { "connected": "0|2|4", "facing": "west" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz" , "y": 270, "uvlock": true}
        },
        {   
            "when": { "connected": "1" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz_corner"}
        },
        {   
            "when": { "connected": "2" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz_corner" , "y": 90, "uvlock": true}
        },
        {   
            "when": { "connected": "4" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz_corner" , "y": 180, "uvlock": true}
        },
        {   
            "when": { "connected": "3" },
            "apply": { "model": "minestrapp:panel_irradiant_quartz_corner" , "y": 270, "uvlock": true}
        },
        {   
            "when": { "facing": "up" },
            "apply": { "model": "minestrapp:polished_irradiant_quartz" }
        }
    ]
}
